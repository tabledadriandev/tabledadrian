{
  "name": "table-d-adrian-wellness",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --webpack",
    "dev:network": "next dev --webpack -H 0.0.0.0",
    "dev:server": "node --max-old-space-size=4096 server.js",
    "build": "prisma generate && next build --webpack",
    "start": "next start",
    "start:custom": "node --max-old-space-size=4096 server.js",
    "lint": "next lint",
    "kill:port": "node scripts/kill-port.js",
    "electron": "npm run kill:port && electron ./electron/main.js",
    "electron:dev": "npm run kill:port && concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron ./electron/main.js\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:linux": "npm run build && electron-builder --linux",
    "electron:pack": "npm run build && electron-builder --dir"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Edge versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "not dead"
  ],
  "dependencies": {
    "@farcaster/frame-sdk": "^0.1.12",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^6.19.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@react-spring/web": "^10.0.3",
    "@stripe/stripe-js": "^2.0.0",
    "@tanstack/react-query": "^5.90.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20.10.4",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.16",
    "gsap": "^3.13.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.294.0",
    "next": "^16.0.3",
    "openai": "^6.9.1",
    "prisma": "^6.19.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.5.0",
    "redis": "^5.10.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^14.0.0",
    "tailwind-merge": "^3.4.0",
    "typescript": "~5.9.2",
    "viem": "^2.39.3",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/cheerio": "^0.22.35",
    "@types/node-cron": "^3.0.11",
    "autoprefixer": "^10.4.16",
    "cheerio": "^1.1.2",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "eslint-config-next": "15.5.6",
    "node-cron": "^4.2.1",
    "png-to-ico": "^3.0.1",
    "postcss": "^8.4.32",
    "sharp": "^0.34.5",
    "string-replace-loader": "^3.3.0",
    "tailwindcss": "^3.3.7",
    "to-ico": "^1.1.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.tabledadrian.wellness",
    "productName": "Table d'Adrian Wellness",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      ".next/standalone/**/*",
      ".next/static/**/*",
      "electron/**/*",
      "public/**/*",
      "package.json",
      "server.js",
      "node_modules/**/*"
    ],
    "extraFiles": [
      {
        "from": "public",
        "to": "public",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraMetadata": {
      "main": "electron/main.js"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "publisherName": "Table d'Adrian",
      "verifyUpdateCodeSignature": false,
      "sign": null
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Table d'Adrian Wellness"
    },
    "portable": {
      "artifactName": "${productName}-${version}-portable.${ext}"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icon.icns",
      "category": "public.app-category.healthcare-fitness"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "build/icon.png",
      "category": "Health"
    }
  }
}
