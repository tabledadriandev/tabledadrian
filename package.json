{
  "name": "ta_app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "replace-token": "node scripts/replace-token-name.js",
    "replace-token:dry": "node scripts/replace-token-name.js --dry-run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:integration": "playwright test",
    "test:integration:ui": "playwright test --ui",
    "test:e2e": "playwright test",
    "security:audit": "npm audit",
    "security:scan": "snyk test || true",
    "security:owasp": "npm run security:scan -- --severity-threshold=high || true",
    "test:a11y": "pa11y http://localhost:3000 || true",
    "test:a11y:ci": "pa11y-ci || true",
    "test:load:normal": "k6 run load-tests/k6-scripts/normal-load.js || true",
    "test:load:peak": "k6 run load-tests/k6-scripts/peak-load.js || true",
    "test:load:stress": "k6 run load-tests/k6-scripts/stress-test.js || true"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@prisma/client": "^7.2.0",
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.12",
    "bcryptjs": "^3.0.3",
    "critters": "^0.0.23",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "next-themes": "^0.4.6",
    "openai": "^6.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "recharts": "^2.10.0",
    "tailwind-merge": "^3.4.0",
    "tesseract.js": "^7.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "daisyui": "^5.5.14",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "pa11y": "^7.0.0",
    "pa11y-ci": "^3.0.1",
    "postcss": "^8.4.0",
    "snyk": "^1.1200.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.0"
  }
}
